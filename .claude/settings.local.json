{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(uv:*)",
      "Bash(export PATH=\"$HOME/.local/bin:$PATH\")",
      "Bash(mkdir:*)",
      "Bash(ls:*)",
      "Bash($HOME/.local/bin/uv.exe:*)",
      "Bash(pip install:*)",
      "Bash(find:*)",
      "Bash(touch:*)",
      "WebFetch(domain:docs.anthropic.com)",
      "Bash(cp:*)",
      "Bash(rm:*)",
      "Bash(echo $HOME)",
      "Bash(echo $HISTFILE)",
      "Bash(cat:*)",
      "Bash(mv:*)",
      "Bash(grep:*)"
    ],
    "deny": []
  },
  "tts_enabled": true
}